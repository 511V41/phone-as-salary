{
  "name": "phone-as-salaly",
  "version": 2,
  "rewrites": [
    { "source": "/(.*)", "destination": "/api/404" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Credentials", "value": "true" },
        { "key": "Access-Control-Allow-Origin", "value": "*" },
        { "key": "Access-Control-Allow-Headers", "value": "*" }
      ]
    }
  ],
  "regions": ["hnd1"],
  "cleanUrls": true,
  "trailingSlash": false
}
